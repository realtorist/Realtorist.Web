{
  "https_port": 443,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "File": {
      "Path": "logs/app.log",
      "Append": "True",
      "FileSizeLimitBytes": 5000000,
      "MaxRollingFiles": 10
    }
  },
  "Extensions": {
    "Path": "Extensions"
  },
  "AllowedHosts": "*"
}
